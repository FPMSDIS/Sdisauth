{
    "name": "fpmsdis/sdisauth",
    "description": "Un package d'authentification bas√© sur Laravel Breeze et Laravel Permission",
    "author": {
        "name": "fpmsdis",
        "email": "fpmsdis@gmail.com",
        "homepage": "https://github.com/fpmsdis",
        "role": "Developper Full Stack PHP"
    },
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sdisauth\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "laravel/breeze": "^2.2",
        "spatie/laravel-permission": "^6.9"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sdisauth\\AuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/fpmsdis/sdisauth"
        }
    ],
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ]
    }
}
